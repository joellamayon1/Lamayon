 public static void main(String[] args) {
        int[] numbers = {7, 3, 7, 1, 5, 7, 2, 8, 7, 0}; // Example array
        int count = 0;

        for (int i = 0; i < numbers.length; i++) {
            if (numbers[i] == 7) {
                count++;
            }
        }

        System.out.println("The number 7 appears " + count + " times.");
    }
}
